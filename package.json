    "build-prod": "npm run clean && cross-env NODE_ENV=prod npm run gulp-build && cross-env NODE_ENV=prod webpack ",
    "clean": "rimraf ./app/assets/dll/* && rimraf ./dist/* && rimraf ./app/library/vendor-*.js",
